#-fsa_fieldsplit_phi_ksp_norm_type unpreconditioned
-fsa_fieldsplit_phi_pc_type gamg
-fsa_fieldsplit_phi_pc_gamg_agg_nsmooths 1
-fsa_fieldsplit_phi_pc_gamg_threshold -0.02
-fsa_fieldsplit_phi_pc_gamg_verbose 0
